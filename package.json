{
  "name": "codex-notebook-extension",
  "displayName": "codex-notebook-extension",
  "description": "Notebook using Serializer API sample",
  "publisher": "vscode-samples",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:codex-type"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codex-notebook-extension.createCodexNotebook",
        "title": "Create Codex Notebook"
      }
    ],
    "notebooks": [
      {
        "type": "codex-type",
        "displayName": "Codex Scripture Editor Notebook",
        "selector": [
          {
            "filenamePattern": "*.codex"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "lint": "eslint src --ext ts",
    "watch": "tsc -b --watch"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^7.27.0",
    "mocha": "^10.2.0",
    "typescript": "^5.3.2"
  }
}